#include<stdio.h>
int func(int number){
    double sum = 0;
    for(double i = 0; i <= number; i++){
        sum+=((2*i + 1.0)/(2*i + 2.0));
    }
    printf("S=%lf",sum);
    return 0;
}
int main(){
    int number = 0;
    printf("Enter number: ");
    scanf("%d", &number);
    func(number);
    return 0;
}
